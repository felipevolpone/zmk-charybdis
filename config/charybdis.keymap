#include <dt-bindings/zmk/mouse.h>
#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/pointing.h>

&lt {
    tapping-term-ms = <200>;
    flavor = "tap-preferred";
    quick-tap-ms = <150>;
};

&mt {
    tapping-term-ms = <240>;
    quick-tap-ms = <140>;
    global-quick-tap;
    flavor = "balanced";
};

/ {
    combos {
        compatible = "zmk,combos";

        tmux-prev-window {
            bindings = <&tmux_prev_window>;
            key-positions = <31 32>;
        };

        tmux-next-window {
            bindings = <&tmux_next_prev>;
            key-positions = <32 33>;
        };

        tmuxnewwindow {
            bindings = <&tmux_new_window>;
            key-positions = <21 22>;
        };

        slash {
            bindings = <&kp SLASH>;
            key-positions = <19 20>;
        };

        shortcat {
            bindings = <&kp LA(LS(LG(S)))>;
            key-positions = <30 31>;
        };

        copy {
            bindings = <&kp LG(C)>;
            key-positions = <38 39>;
        };

        paste {
            bindings = <&kp LG(V)>;
            key-positions = <39 40>;
        };
    };

    macros {
        tmux_new_window: tmux_new_window {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LC(A) &kp C>;
            label = "TMUX_NEW_WINDOW";
        };

        tmux_prev_window: tmux_prev_window {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LC(A) &kp H>;
            label = "TMUX_PREV_WINDOW";
        };

        tmux_next_prev: tmux_next_prev {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LC(A) &kp N>;
            label = "TMUX_NEXT_PREV";
        };
    };

    keymap {
        compatible = "zmk,keymap";

        l_qwerty {
            bindings = <
&kp GRAVE   &kp N1      &kp N2  &kp N3        &kp N4      &kp N5    &kp N6     &kp N7           &kp N8        &kp N9   &kp N0              &kp LG(SPACE)
&none       &kp Q       &kp W   &kp E         &kp R       &kp T     &kp Y      &kp U            &kp I         &kp O    &kp P               &none
&kp LSHIFT  &mt LGUI A  &kp S   &mt LSHIFT D  &kp F       &kp G     &kp H      &kp J            &mt LSHIFT K  &kp L    &mt LGUI SEMICOLON  &mo 3
&kp LCTRL   &kp Z       &kp X   &kp C         &kp V       &kp B     &kp N      &kp M            &kp COMMA     &kp DOT  &kp QMARK           &kp RGUI
                                &lt 2 SPACE   &kp ESCAPE  &none     &kp ENTER  &lt 1 BACKSPACE
                                              &mkp LCLK   &none     &mo 3
            >;

            trackball-bindings = <&tmv_coarse>;
        };

        nav {
            bindings = <
&trans  &trans  &trans                &trans                 &trans              &trans                 &trans      &trans                &trans         &trans  &trans  &trans
&trans  &trans  &kp LG(LEFT_BRACKET)  &kp LG(RIGHT_BRACKET)  &kp LG(LEFT_BRACE)  &kp LG(RIGHT_BRACE)    &kp N0      &kp LA(LEFT)          &kp LA(RIGHT)  &none   &none   &trans
&trans  &trans  &kp C_VOLUME_DOWN     &kp C_MUTE             &kp C_VOLUME_UP     &kp LG(LEFT)           &caps_word  &kp TAB               &kp LS(TAB)    &none   &none   &trans
&trans  &trans  &trans                &kp C_PLAY_PAUSE       &trans              &kp LG(RIGHT)          &trans      &kp LG(LS(NUMBER_4))  &none          &none   &trans  &trans
                                      &trans                 &trans              &trans                 &trans      &trans
                                                             &trans              &trans                 &trans
            >;
        };

        symbols {
            bindings = <
&trans  &trans     &trans          &trans                &trans                 &trans             &trans        &trans             &trans         &trans         &trans             &to 0
&trans  &kp GRAVE  &kp MINUS       &kp LEFT_BRACE        &kp RIGHT_BRACE        &kp EXCLAMATION    &kp ASTERISK  &kp DOLLAR         &kp PERCENT    &kp SQT        &kp DOUBLE_QUOTES  &trans
&trans  &kp PIPE   &kp PLUS        &kp LEFT_PARENTHESIS  &kp RIGHT_PARENTHESIS  &kp AT_SIGN        &kp LEFT      &kp DOWN           &kp UP_ARROW   &kp RIGHT      &kp TILDE          &trans
&trans  &trans     &kp UNDERSCORE  &kp LEFT_BRACKET      &kp RIGHT_BRACKET      &kp HASH           &kp COLON     &kp EQUAL          &kp AMPERSAND  &kp BACKSLASH  &kp LCTRL          &trans
                                   &trans                &trans                 &trans             &none         &kp LA(BACKSPACE)
                                                         &trans                 &trans             &trans
            >;
        };

        mouse {
            bindings = <
&trans  &trans  &trans  &trans        &trans          &trans    &trans  &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans        &trans          &trans    &trans  &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &msc SCRL_UP  &msc SCRL_DOWN  &trans    &trans  &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans        &trans          &trans    &trans  &trans  &trans  &trans  &trans  &trans
                        &trans        &trans          &trans    &trans  &trans
                                      &trans          &trans    &trans
            >;
        };

